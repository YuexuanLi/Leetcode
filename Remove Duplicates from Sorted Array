//https://oj.leetcode.com/problems/remove-duplicates-from-sorted-array/
public class Solution {
    public static int removeDuplicates(int[] A) {
        int count = 0;
        
        if (A.length < 2){
            return A.length;
        }
        
        for (int i = 1; i < A.length; i++){
            if (A[i] == A[i-1]){
                count++;
            }else{
                A[i-count] = A[i];
            }
        }
        
        return A.length - count;
    }
}
